<Application x:Class="MvvmTemplate.GUI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MvvmTemplate.GUI"
             Startup="App_OnStartup" Exit="App_OnExit">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <SharedResourceDictionary Source="/MvvmTemplate.GUI;component/Views/Styles/General.xaml"/>
                
                <SharedResourceDictionary Source="/MvvmTemplate.GUI;component/Views/Infos/ReleaseNotesV.xaml"/>
                <SharedResourceDictionary Source="/MvvmTemplate.GUI;component/Views/Infos/NotesV.xaml"/>
                
                <SharedResourceDictionary Source="/MvvmTemplate.GUI;component/Views/Messages/ErrorMessageV.xaml"/>
                <SharedResourceDictionary Source="/MvvmTemplate.GUI;component/Views/Messages/TempInfoMessageV.xaml"/>
                <SharedResourceDictionary Source="/MvvmTemplate.GUI;component/Views/Messages/InfoMessageV.xaml"/>
                <SharedResourceDictionary Source="/MvvmTemplate.GUI;component/Views/Messages/MessagesV.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
